<?xml version="1.0" encoding="UTF-8"?>
<app-interfaces>
	
	<interface name="getSmsCode">
		<description><![CDATA[获取手机短信验证码]]></description>
		<parameters>
			<param name="mobile" type="MOBILE" required="true"/>
		</parameters>
		<result datatype="json">
			<status>OK|ERROR</status>
			<retCode valuetype="int" />
			<message valuetype="String" />
		</result>
	</interface>
	
